<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.qhit.lh.g4.wf.T1.bean.user" table="t_user">
	    <id name="id" column="id" type="integer">
			<generator class="native">
			</generator>
		</id>
		<property name="uname" column="uname" type="string"></property>
		<property name="upwd" column="upwd" type="string"></property>
		<property name="birthday" column="birthday" type="string"></property>
		<property name="sex" column="sex" type="string"></property>
		<property name="uenable" column="uenable" type="integer"></property>
	</class>
</hibernate-mapping>
